{
	"name": "appleblox",
	"version": "0.9.0-dev.36",
	"description": "Lightweight and fast Roblox launcher for MacOS",
	"scripts": {
		"vite:dev": "vite",
		"dev": "bun run scripts/package/dev.ts",
		"build": "bun run scripts/build/ts/parallel-build.ts",
		"build:sequential": "bun run scripts/build/ts/index.ts",
		"build:arm64": "BUILD_ARCH=arm64 bun run scripts/build/ts/index.ts",
		"build:x64": "BUILD_ARCH=x64 bun run scripts/build/ts/index.ts",
		"build:universal": "BUILD_ARCH=universal bun run scripts/build/ts/index.ts",
		"build:all": "bun run scripts/build/ts/parallel-build.ts",
		"build:clean": "rm -rf dist .tmpbuild bin",
		"dmg": "bun run scripts/build/ts/dmg.ts",
		"dmg:arm64": "BUILD_ARCH=arm64 bun run scripts/build/ts/dmg.ts",
		"dmg:x64": "BUILD_ARCH=x64 bun run scripts/build/ts/dmg.ts",
		"dmg:universal": "BUILD_ARCH=universal bun run scripts/build/ts/dmg.ts",
		"release": "bun run build && bun run dmg",
		"release:sequential": "bun run build:sequential && bun run dmg:arm64 && bun run dmg:x64 && bun run dmg:universal",
		"release:arm64": "bun run build:arm64 && bun run dmg:arm64",
		"release:x64": "bun run build:x64 && bun run dmg:x64",
		"release:universal": "bun run build:universal && bun run dmg:universal",
		"test": "bun test",
		"test:unit": "bun test",
		"test:watch": "bun test --watch",
		"test:coverage": "bun test --coverage",
		"format": "prettier --write ./",
		"neu:empty": ""
	},
	"keywords": [
		"svelte",
		"neutralino",
		"webdev",
		"ts",
		"roblox",
		"macos"
	],
	"type": "module",
	"author": "OrigamingWasTaken",
	"license": "GPL-3.0-only",
	"devDependencies": {
		"@neutralinojs/lib": "^6.2.0",
		"@neutralinojs/neu": "^11.6.0",
		"@sveltejs/vite-plugin-svelte": "^3",
		"@tsconfig/svelte": "^5.0.6",
		"@types/dompurify": "^3.2.0",
		"@types/opentype.js": "^1.3.8",
		"@types/path-browserify": "^1.0.2",
		"@types/semver-compare": "^1.0.3",
		"@types/signale": "^1.4.7",
		"@types/tar": "^6.1.11",
		"autoprefixer": "^10.4.22",
		"minimatch": "^10.1.1",
		"postcss": "^8.5.6",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"signale": "^1.4.0",
		"svelte": "^4",
		"svelte-check": "^3",
		"tailwindcss": "^3",
		"tar": "^6.2.0",
		"terser": "^5.44.1",
		"tslib": "^2.6.2",
		"typescript": "^5.9.3",
		"vite": "^5",
		"vite-plugin-checker": "^0.11.0",
		"vite-plugin-html": "^3.2.2"
	},
	"dependencies": {
		"@tailwindcss/typography": "^0.5.19",
		"@types/bun": "^1.3.3",
		"bits-ui": "^0.21.16",
		"clsx": "^2.1.1",
		"cmdk-sv": "^0.0.19",
		"dompurify": "^3.3.0",
		"hotkeys-js": "^4.0.0-beta.7",
		"json-beautify": "^1.1.1",
		"lucide-svelte": "^0.554.0",
		"marked": "^17.0.1",
		"mode-watcher": "^0.3.1",
		"opentype.js": "^1.3.4",
		"path-browserify": "^1.0.1",
		"semver-compare": "^1.0.0",
		"svelte-awesome-color-picker": "3",
		"svelte-sonner": "^0.3.21",
		"tailwind-merge": "^2",
		"tailwind-variants": "^0.2.1"
	},
	"trustedDependencies": [
		"bufferutil",
		"core-js",
		"es5-ext",
		"esbuild",
		"svelte-preprocess",
		"utf-8-validate"
	]
}
